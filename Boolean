/* Tipo booleano */
let isAtivo = false
console.log(isAtivo)

isAtivo = true
console.log(isAtivo)

isAtivo = 1
/*Como transformar um número em verdadeiro ou falso? 
! = negação
!! = verdadeiro*/
console.log(!!isAtivo)

console.log('Os verdadeiros...')
console.log(!!3)
console.log(!!Infinity)
console.log(!!('' || null || 0 || ''))
